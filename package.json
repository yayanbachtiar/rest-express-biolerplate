{
  "name": "apiv2",
  "version": "0.0.1",
  "description": "boilerplate nodejs express babel docker",
  "main": "src/app.js",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "pretest": "eslint .",
    "start": "nodemon src/app.js --exec babel-node",
    "build": "babel src -d dist --ignore **/__tests__/**,**/__mocks__/**",
    "_codecov": "codecov",
    "_heroku": "heroku",
    "lint": "eslint 'src/**/*.js'",
    "unit": "jest \"src/.*\\.js$\" --coverage --no-watchman --detectOpenHandles --forceExit",
    "e2e": "cross-env JEST_ENV=e2e jest \"src/.*\\.js$\" --no-watchman --detectOpenHandles --forceExit",
  },
  "author": "subchan yayan bachtiar",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "eslint-config-airbnb": "^17.1.1",
    "express": "^4.17.1",
    "express-slow-down": "^1.3.1",
    "helmet": "^3.18.0",
    "passport": "^0.4.0",
    "sequelize": "^5.8.7",
    "swagger-jsdoc": "^3.2.9",
    "swagger-ui-express": "^4.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-plugin-root-import": "^6.2.0",
    "eslint": "^6.1.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2"
  }
}
